<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - REALLY9</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/antd/dist/antd.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/adminassets/adminboost/lib/bootstrap/dist/css/bootstrap.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/adminassets/adminboost/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/REALLY9.styles.css" asp-append-version="true" />
    <!-- Favicon -->
    <link href="~/adminassets/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/antd/dist/antd.min.css" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/adminassets/adminboost/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/adminassets/adminboost/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />
    
    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/adminassets/adminboost/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/adminassets/adminboost/css/style.css" rel="stylesheet">
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">REALLY9</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Admin" asp-controller="Home" asp-action="Index">ADMIN</a>
                        </li>
                        
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

   
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>






    <script src="~/adminassets/adminboost/lib/chart/chart.min.js"></script>
    <script src="~/adminassets/adminboost/lib/easing/easing.min.js"></script>
    <script src="~/adminassets/adminboost/lib/easing/easing.js"></script>
    <script src="~/adminassets/adminboost/lib/waypoints/waypoints.min.js"></script>
    <script src="~/adminassets/adminboost/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/adminassets/adminboost/lib/owlcarousel/owl.carousel.js"></script>
    <script src="~/adminassets/adminboost/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/adminassets/adminboost/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/adminassets/adminboost/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/adminassets/adminboost/lib/tempusdominus/js/tempusdominus-bootstrap-4.js"></script>

    <!-- Template Javascript -->
    <script src="~/adminassets/adminboost/js/main.js"></script>
    @await Component.InvokeAsync("NToastNotify")

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
